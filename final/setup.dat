<span class="instructionsheading">Game Setup:</span>
<form id="gamesetupform">
	<input type="hidden" name="gamename" id="changegamename" value="Wildcard Cricket">
	<input id='lat' type="hidden" name="latitude" value="0">
	<input id='long' type="hidden" name="longitude" value="0">
	<input id='currtime' type="hidden" name="currentlocaltime" value="0">
	<input id='currdate' type="hidden" name="currentdate" value="0">
	<div id="gamesetuptable">
        <div id = 'selectgamestyle' style="text-align: center;">Select a Game Style</div>
        <div class='gamestyle'><button id="button1">Wildcard Cricket</button></div>
        <div class='gamestyle'><button id="button2">301</button></div>
        <div id="gamesetuptablesubmitbutton" style='text-align:center;'>
            <div>
                <input id="userstartgamebutton" type="submit" name="submit" value="Start Game">
            </div>
        </div>
	</div>
</form>
